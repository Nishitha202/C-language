#include <stdio.h>
#include<string.h>
#include<ctype.h>
int main() {
    // Write C code here

char str[]="Good Morning";
int n=strlen(str);

for(int i=0;i<n;i++){
    
    if(str[i]>='a' && str[i]<='z'){
        str[i]=toupper(str[i]);

    }else{
         str[i]=tolower(str[i]);

    }
}

for(int i=0;i<n;i++){
printf("%c",str[i]);
}
    return 0;
}
